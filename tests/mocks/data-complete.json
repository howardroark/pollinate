{
  "name": "data-complete",
  "image": "debian",
  "discard": [
    "README.md"
  ],
  "parse": [
    "Dockerfile",
    "PROJECT-README"
  ],
  "move": [
   { "PROJECT-README": "README.md" },
   { "project-name": "{{ name }}.txt" },
   { "project-name-dir": "{{ name }}" },
   { "project-name-nested": "{{ name }}/{{ name }}/{{ name }}.txt" }
  ],
  "complete": "git init {{ name }}"
}
